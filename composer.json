{
    "name": "fruitriin/lucienne",
    "description": "My own framework",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "FruitRiin\\Lucienne\\": "src/"
        }
    },
    "authors": [
        {
            "name": "FruitRiin",
            "email": "nassii74@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "slim/http": "^1.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload-dev": {
        "psr-4": {
            "Riin\\Lucienne\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "homepage": "https://github.com/fruitriin/lucienne",
    "keywords": [
        "framework",
        "web",
        "mvc"
    ]
}
